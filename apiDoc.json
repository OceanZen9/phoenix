            "type": "string"
          },
          "accessToken": {
            "type": "string"
          }
        },
        "required": [
          "refreshToken",
          "accessToken"
        ]
      },
      "LogoutPayload": {
        "type": "object",
        "properties": {
          "refreshToken": {
            "type": "string"
          }
        },
        "required": [
          "refreshToken"
        ]
      },
